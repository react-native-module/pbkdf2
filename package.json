{
  "author": "sacru2red <sacru2red@gmail.com>",
  "name": "@react-native-module/pbkdf2",
  "description": "implementation of PBKDF2 for React Native",
  "version": "0.3.3",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": {
    "pbkdf2": false
  },
  "source": "src/index",
  "homepage": "https://github.com/react-native-module/pbkdf2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-native-module/pbkdf2.git"
  },
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "rimraf ./lib && rollup -c ./rollup.config.js && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir lib/typescript",
    "release": "yarn build && release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "dependencies": {
    "@react-native-module/utility": "0.0.4-0",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "pbkdf2": "^3.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "3.0.3",
    "@release-it/conventional-changelog": "5.0.0-esm.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^26.0.0",
    "commitlint": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "release-it": "15.0.0-esm.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "react native",
    "pbkdf2",
    "Crypto.pbkdf2",
    "ios",
    "android",
    "web"
  ],
  "bugs": {
    "url": "https://github.com/react-native-module/pbkdf2/issues"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-module-pbkdf2.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
